let empty_arr: array[int] = [];
assert(empty_arr.len == 0);
assert(f"{empty_arr}" == "[]");
assert(f"{[1, 2, 3]}" == "[1, 2, 3]");
let arr = [1];
assert(arr.len == 1);
assert(arr.pop() == 1);
assert(f"{arr}" == "[]");
arr.push(2);
assert(f"{arr}" == "[2]");
assert([1][0] == 1);

let unsorted = [5, 2, 6, 9, 2, 1];
assert(f"{unsorted.sort()}" == f"{[1, 2, 2, 5, 6, 9]}");
